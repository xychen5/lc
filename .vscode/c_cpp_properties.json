    {
      "name": "Win32",
      "includePath": ["${workspaceFolder}/**"],
      "defines": ["_DEBUG", "UNICODE", "_UNICODE"],
      "compilerPath": "cl.exe",
      "cStandard": "c17",
      "cppStandard": "c++17"
    }
  ],
  "version": 4
}
